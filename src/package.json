  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules ./ClientApp/scss -o ./ClientApp/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./ClientApp/scss -o ./ClientApp/css --watch --recursive",
    "build:dev": "node ./node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js --mode development && node ./node_modules/webpack/bin/webpack.js --config webpack.config.js --mode development",
    "build:prod": "node ./node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js --mode production && node ./node_modules/webpack/bin/webpack.js --config webpack.config.js --mode production",
    "build": "npm-run-all build-css build:prod",
    "start": "npm-run-all -p watch-css build:dev"
  },
